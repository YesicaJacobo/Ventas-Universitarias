        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.352" />

    <Button
        android:id="@+id/creartienda"
        android:layout_width="192dp"
        android:layout_height="48dp"
        android:background="@drawable/border_redondos"
        android:backgroundTint="#751F1F"
        android:text="@string/crear_tienda_crear"
        android:textAppearance="@style/TextOtro"
